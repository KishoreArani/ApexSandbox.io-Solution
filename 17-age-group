public String ageGroup(Integer n) {
    //code here
    String ans = '';
    if(n >= 0 && n <= 1){
        ans = 'Infant';
    }
    else if(n >= 2 && n <= 14){
        ans = 'Child';
    }
    else if(n >= 15 && n <= 21){
        ans = 'Youth';
    }
    else if(n >= 22){
        ans = 'Adult';
    }

    return ans;
}