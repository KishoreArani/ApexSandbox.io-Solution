public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> result = new List<sObject>();

    if(accounts.size() > 0){
        for(sObject sobj : accounts){
            result.add(sobj);
        }
    }

    if(contacts.size() > 0){
        for(sObject sobj : contacts){
            result.add(sobj);
        }
    }

    return result;
}